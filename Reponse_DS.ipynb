    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "def f1(x):\n",
    "   f1 = x**4.0\n",
    "   return f1;\n",
    "\n",
    "def f2(x):\n",
    "   f2 = np.exp(-x*x)\n",
    "   return f2;\n",
    "\n",
    "def f3(x):\n",
    "   tau   = 1.0e-8\n",
    "   f3    = np.where(np.abs(x)<tau,1.0,np.sin(x)/x)\n",
    "   return f3;"
   ]
